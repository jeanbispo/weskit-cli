{
  "name": "weskit-cli",
  "version": "1.0.0",
  "description": "Weskit is an template for use to start the development of web projects.",
  "repository": "https://github.com/jeanbispo/weskit-cli.git",
  "author": "Jean Bispo",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "nodemon --watch 'lib/**/*.ts' --exec 'ts-node' ./bin/weskit",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "test": "sudo npm i -g && weskit",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "bin": {
    "weskit": "./bin/weskit"
  },
  "engines": {
    "node": ">=10.13.0 <13.0.0",
    "yarn": ">=1.17.3 <2.0.0"
  },
  "dependencies": {
    "@babel/preset-env": "^7.8.4",
    "browser-sync": "^2.26.7",
    "browserify": "^16.5.0",
    "chalk": "^3.0.0",
    "clear": "^0.1.0",
    "commander": "^4.1.1",
    "del": "^5.1.0",
    "figlet": "^1.2.4",
    "fs": "^0.0.1-security",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-if": "^3.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "inquirer": "^7.0.4",
    "shelljs": "^0.8.3",
    "vinyl-source-stream": "^2.0.0",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@types/node": "^13.7.1",
    "babelify": "^10.0.0",
    "gulp-debug": "^4.0.0",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
